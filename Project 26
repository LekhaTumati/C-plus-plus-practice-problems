#include <iostream>

using namespace std;

void shift_right(float a, float b, float c)
{
    float MyArray[3] = {a, b, c};

    MyArray[0] = b;
    MyArray[1] = c;
    MyArray[2] = a;
    if(b <= c)
    {
        for(int i = 0; i <= 2; i++)
        {
            cout << MyArray[i] << endl;
        }
    }
}

int main()
{
    shift_right(1, 2, 3);
}
